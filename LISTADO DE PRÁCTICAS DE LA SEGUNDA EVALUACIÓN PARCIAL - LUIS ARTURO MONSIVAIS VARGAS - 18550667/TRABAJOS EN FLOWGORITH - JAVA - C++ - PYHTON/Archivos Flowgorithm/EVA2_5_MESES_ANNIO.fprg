                                                                </then>
                                                                <else>
                                                                    <if expression="iNumero == 5">
                                                                        <then>
                                                                            <output expression="&quot;Mayo&quot;" newline="True"/>
                                                                        </then>
                                                                        <else>
                                                                            <if expression="iNumero == 4">
                                                                                <then>
                                                                                    <output expression="&quot;Abril&quot;" newline="True"/>
                                                                                </then>
                                                                                <else>
                                                                                    <if expression="iNumero == 3">
                                                                                        <then>
                                                                                            <output expression="&quot;Marzo&quot;" newline="True"/>
                                                                                        </then>
                                                                                        <else>
                                                                                            <if expression="iNumero == 2">
                                                                                                <then>
                                                                                                    <output expression="&quot;Febrero&quot;" newline="True"/>
                                                                                                </then>
                                                                                                <else>
                                                                                                    <if expression="iNumero == 1">
                                                                                                        <then>
                                                                                                            <output expression="&quot;Enero&quot;" newline="True"/>
                                                                                                        </then>
                                                                                                        <else>
                                                                                                            <if expression="iNumero &gt; 12">
                                                                                                                <then>
                                                                                                                    <output expression="&quot;Numero fuera de rango&quot;" newline="True"/>
                                                                                                                </then>
                                                                                                                <else>
                                                                                                                    <if expression="iNumero &lt; 1">
                                                                                                                        <then>
                                                                                                                            <output expression="&quot;Numero fuera de rango&quot;" newline="True"/>
                                                                                                                        </then>
                                                                                                                        <else/>
                                                                                                                    </if>
                                                                                                                </else>
                                                                                                            </if>
                                                                                                        </else>
