    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Inserta un numero de mes&quot;" newline="True"/>
            <declare name="iNumero" type="Integer" array="False" size=""/>
            <input variable="iNumero"/>
            <if expression="iNumero == 12">
                <then>
                    <output expression="&quot;Diciembre&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="iNumero == 11">
                        <then>
                            <output expression="&quot;Noviembre&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="iNumero == 10">
                                <then>
                                    <output expression="&quot;Octubre&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="iNumero == 9">
                                        <then>
                                            <output expression="&quot;Septiembre&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="iNumero == 8">
                                                <then>
                                                    <output expression="&quot;Agosto&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="iNumero == 7">
                                                        <then>
                                                            <output expression="&quot;Julio&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <if expression="iNumero == 6">
                                                                <then>
